    <title>HOTEL</title>
    <link href="~/Content/Hotel/css/bootstrap.css" rel="stylesheet">
    <link href="~/Content/Hotel/css/magnific-popup.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Content/Hotel/css/animate-custom.css">
    <link href="~/Content/Hotel/css/prettyPhoto.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Hotel/css/hover-min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/Content/Hotel/css/font-awesome.css">
    <link href="~/Content/Hotel/css/navigation-styles.css" rel="stylesheet">
    <link href="~/Content/Hotel/css/jquery.bxslider.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Hotel/css/styles.css" rel="stylesheet">
    @RenderSection("csss", required: false)

