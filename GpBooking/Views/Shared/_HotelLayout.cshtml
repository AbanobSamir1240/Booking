</head>
<body data-spy="scroll" data-target=".navMenuCollapse">

    <div id="wrap">

        <section  style="background-image: url(@RenderSection("img", required: true));"  class="module parallax ewebcraft-banner5" style="height: 100vh;">
            <div class="container">
                <div class="row">

                    <div class="col-sm-12 banner-block  text-center">
                        <h1>
                            @RenderSection("HotelName", required: true)
                        </h1>
                        <h2>ENJOY WITH US</h2>
                        <hr>
                    </div>
                </div>
            </div>
        </section>
        <!-- Content start -->
        @RenderBody()
        <section id="ewc-footer-6">
            <div class="container">
                <div class="row text-center align-items-center align-items-md-center align-content-center">
                    <div class="social_icon">
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-facebook"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-google"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-twitter"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-linkedin"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-dribbble"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-instagram"></i></a>
                        </div>
                        <div class="icon hvr-wobble-bottom hvr-fade">
                            <a href="#" class="hvr-wobble-bottom hvr-fade"><i class="fa fa-youtube-play"></i></a>
                        </div>

                    </div>
                </div>
            </div>
        </section>
    </div><!-- /#wrap -->
    <!-- JavaScript -->
    <script src="~/Scripts/Hotel/jquery.js" type="text/javascript"></script>
    <script src="~/Scripts/Hotel/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Hotel/smoothScroll.js" type="text/javascript"></script>
    <script src="~/Scripts/Hotel/waypoints.js" type="text/javascript"></script>
    @*<script src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
        <script src="http://maps.googleapis.com/maps-api-v3/api/js/22/5a/main.js"></script>*@
