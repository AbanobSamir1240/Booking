    <script src="~/Scripts/Hotel/jquery.bxslider.js" type="text/javascript"></script>
    <script src="~/Scripts/Hotel/jquery.counterup.js" type="text/javascript"></script>
    <script src="~/Scripts/Hotel/script.js" type="text/javascript"></script>

